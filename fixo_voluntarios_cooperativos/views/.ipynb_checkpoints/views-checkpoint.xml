<odoo>
    <data>
        
        <record model="ir.ui.view" id="fixo_tareas_view_form"> 
            <field name="name">fixo_voluntarios_cooperativos.tareas.form</field>
            <field name="model">fixo_voluntarios_cooperativos.tareas</field>
            <field name="arch" type="xml">
                <form string="Course Form">
                    <sheet>
                        <h1><field name="name"></field></h1>
                        <group>
                            <group>
                                <field name="horaInicio"></field>
                            </group>
                            <group>
                                <field name="horaFIn"></field>
                            </group>
                        </group>
                        <group>
                            <group>
                                <field name="periorica"></field>
                            </group>
                            <group>
                                <field name="periodicidad"></field>
                            </group>
                        </group>
                        
                    </sheet>
                </form>
            </field>
        </record>
        
        <record model="ir.ui.view" id="fixo_tareas_view_list"> 
            <field name="name">fixo_voluntarios_cooperativos.tareas.list</field>
            <field name="model">fixo_voluntarios_cooperativos.tareas</field>
            <field name="arch" type="xml">
                <tree string="Tareas List">
                    <field name="name"></field>
                    <field name="horaInicio"></field>
                    <field name="horaFIn"></field>
                    <field name="periorica"></field>
                    <field name="periodicidad"></field>
                </tree>
            </field>
        </record>
        
        <record model="ir.ui.view" id="fixo_tareas_view_search"> 
            <field name="name">fixo_voluntarios_cooperativos.tareas.search</field>
            <field name="model">fixo_voluntarios_cooperativos.tareas</field>
            <field name="arch" type="xml">
                <search>
                    <field name="name"></field>
                    <field name="periorica"></field>
                </search>
            </field>
        </record>
            
    </data>
</odoo>